function Generate-Alarm
{
	<#
	.SYNOPSIS
	Generates an alarm that lasts for 60 seconds.
	#>
	$flag = $true
	while ($flag -eq $true)
	{
		$sound.PlayLooping()
		sleep -Seconds 60
		$flag = $false
	}
	$sound.Stop()
}
